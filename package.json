{
  "name": "cocina-pacense",
  "version": "3.12.0",
  "private": true,
  "description": "Este es el portal web de las jornadas de cocina tradicional de Badajoz.",
  "scripts": {
    "parcel:serve": "parcel -p 8123 --target web",
    "parcel:build": "parcel build --target web --no-source-maps --no-cache",
    "clean": "rimraf dist .cache .cache-loader .parcel-cache",
    "dev": "npm-run-all clean parcel:serve",
    "build": "npm-run-all clean parcel:build",
    "test": "echo 'Everything is working as expected âœ…\nStart working on your project by running \\033[1mnpm run dev\\033[0m'",
    "stylelint": "stylelint src/**/*.scss"
  },
  "targets": {
    "web": {
      "context": "browser",
      "publicUrl": "./",
      "source": "src/index.html",
      "engines": {
        "browsers": "last 2 versions, > 0.5%, not dead"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dacorder/herramientas-html-y-css-2-pec-3"
  },
  "author": {
    "name": "David Cordero Ramirez",
    "email": "dacorder@uoc.edu"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uoc-advanced-html-css/uoc-boilerplate/issues"
  },
  "homepage": "https://cocinapacense2.netlify.app/",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.15.3",
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.15.3",
    "postcss-preset-env": "^10.1.3",
    "posthtml-include": "^2.0.1",
    "rimraf": "^6.0.1",
    "sharp": "^0.33.5",
    "stylelint": "^16.26.1",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-scss": "^6.14.0",
    "tailwindcss": "^4.1.18"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.1"
  }
}
